const Null = 512
global M = array [Null * 2]
global consSP // SP means "stack pointer"
global ax, dx, depth, panic, fail
global kEq, kCar, kCdr, kCond, kAtom, kCons, kQuote, kDefine

def Get(i) {
  M[Null + i]
}

def Set(i, x) {
  M[Null + i] = x
}

def Car(x) {
  Get(x)
}

def Cdr(x) {
  Get(x + 1)
}

def Cons(car, cdr) {
  Set(--cx, cdr)
  Set(--cx, car)
  cx
}


